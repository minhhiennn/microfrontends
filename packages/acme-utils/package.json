{
  "name": "@acme/utils",
  "version": "0.0.1",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "tsup src/* --format esm,cjs --dts --watch --external next",
    "build": "tsup src/* --format esm,cjs --dts --external next",
    "lint": "TIMING=1 eslint \"**/*.{ts,tsx,js,jsx}\" --fix",
    "clean": "rm -rf dist && rm -rf .turbo"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./*": {
      "import": [
        "./dist/*.js",
        "./dist/*/index.js"
      ],
      "require": [
        "./dist/*.cjs",
        "./dist/*/index.cjs"
      ]
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "src/*",
        "src/*/index"
      ]
    }
  },
  "publishConfig": {
    "typesVersions": {
      "*": {
        "*": [
          "dist/*.d.ts",
          "dist/*/index.d.ts"
        ]
      }
    }
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "next": "^14.2.0"
  },
  "dependencies": {
    "axios": "^1.6.7"
  },
  "devDependencies": {
    "@types/axios": "^0.14.4",
    "@types/node": "^20.11.19",
    "eslint": "^8.16.0",
    "eslint-config-acme": "workspace:*",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  }
}
